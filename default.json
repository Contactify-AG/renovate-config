{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":semanticCommits",
    "group:allNonMajor",
    ":enableVulnerabilityAlertsWithLabel(High priority)"
  ],
  "rebaseStalePrs": true,
  "dependencyDashboard": false,
  "schedule": [
    "every 1 hour"
  ],
  "pre-commit": {
    "enabled": true
  }
}
